{
  "name": "seneca-entity",
  "main": "dist/entity.js",
  "version": "25.1.3",
  "type": "commonjs",
  "types": "dist/entity.d.ts",
  "description": "Entity plugin for seneca",
  "keywords": [
    "seneca",
    "entity",
    "plugin"
  ],
  "license": "MIT",
  "homepage": "http://senecajs.org",
  "author": "Richard Rodger (http://richardrodger.com/)",
